{
  "extends": "solhint:default",
    "plugins": [],
    "rules": {
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "compiler-version" :[ "error", "0.8.0", "0.6.0"],
      "func-visibility": ["warn", {"ignoreConstructo": true}],
      "var-name-mixedcase": "off",
      "avoid-low-level-calls" : "off"
    }
}
